<mvc:View
	controllerName="com.christian.ui.location.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true">
	<App>
		<pages>
			<Page title="{i18n>homePageTitle}">
				<content>
					<HBox items="{/users}">
						<items>
							<Button
								text="{test}"
								press=".onShowHello"/>
						</items>
					</HBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>